<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_cebanc_kebab_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.alumno.cebanckebab.CebancKebabMainActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/textTitulo"
        android:textSize="30sp"
        android:id="@+id/textTitulo"
        android:gravity="center_horizontal"
        android:layout_marginBottom="10sp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/textContacto"
        android:textSize="20sp"
        android:id="@+id/textContacto"
        android:layout_marginBottom="20sp"
        />

    <Button
        android:text="@string/buttonIniciar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/buttonIniciar" />

</LinearLayout>
