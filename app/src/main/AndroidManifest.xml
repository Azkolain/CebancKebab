<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alumno.cebanckebab">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CebancKebabMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_1_datos"
            android:label="@string/app_name_datos"/>
        <activity android:name=".activity_2_tipo"
            android:label="@string/app_name_tipo"/>
        <activity android:name=".activity_3_bebidas"
            android:label="@string/app_name_bebidas"/>
        <activity android:name=".activity_4_resumen"
            android:label="@string/app_name_resumen"/>
    </application>

</manifest>